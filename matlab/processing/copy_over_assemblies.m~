for ins=1:h
    
    if ~pipevar.redo(ins),  continue; end
    
    SS=SameStrains(SameStrains.InstructionsRef==ins,:);
    for i = 1:height(SS)
        %make dir
        sp=SS.seqPlates{i};
        pipevar.assembly_folder{ins}

        
        for i=1:numel(sp)
            mkdir([Path.assembly dl GlobalName sp])
            ['cp -u
        end
    end
end